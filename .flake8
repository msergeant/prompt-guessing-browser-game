[flake8]
exclude =
    # No need to traverse our git directory
    .git,
    # There's no value in checking cache directories
    __pycache__,
    # ignore autogenerated migrations
    */migrations/*,
    # ignore boilerplate django
    manage.py,
    bro/*settings*,
    */apps.py

    # ignore unused variables in collector files.
    intel/catalogue.py
    resource/management/commands/generate_missing_thumbnails.py
    **/__init__.py
    .venv/
    ./integrations/partners/jakentra/tests/fixtures/expected_serialized_response.py
max-complexity = 26
max-line-length = 120
